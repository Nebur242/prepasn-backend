{
  "root": "apps/hls",
  "sourceRoot": "apps/hls/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/hls",
        "main": "apps/hls/src/main.ts",
        "tsConfig": "apps/hls/tsconfig.app.json"
      },
      "configurations": {
        "serverless": {
          "main": "apps/hls/src/serverless.ts",
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "generatePackageJson": true,
          "progress": true
        },
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "hls:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/hls/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/hls"],
      "options": {
        "jestConfig": "apps/hls/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
